<% if user_signed_in? %>
	<%= link_to "Bookings", bookings_path, class: "nav-item nav-link" %>
	<%= link_to 'Settings', edit_user_registration_path, class: "nav-item nav-link" %>
	<%= link_to 'Log out', destroy_user_session_path, method: :delete, class: "nav-item nav-link" %>
<% else %>
	<%= link_to "Log in", new_user_session_path, class: "nav-item nav-link" %>
	<%= link_to "Register ", new_user_registration_path, class: "nav-item nav-link" %>
<% end %>